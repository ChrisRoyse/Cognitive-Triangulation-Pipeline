{
  "name": "aback",
  "version": "1.0.0",
  "description": "Automated Backend Analysis and Code Knowledge - Cognitive Triangulation Architecture",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "start:api": "node src/utils/pipelineApi.js",
    "test": "jest",
    "test:e2e": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:polyglot": "node run-polyglot-tests.js",
    "test:cognitive": "node run-tests.js",
    "check:polyglot": "node check-polyglot-results.js",
    "init-db": "node src/utils/initializeDb.js",
    "test:neo4j:ip": "cross-env NEO4J_URI=bolt://127.0.0.1:7687 node test_neo4j_connection.js",
    "test:pipeline": "node test-pipeline.js",
    "test:pipeline:verbose": "node test-pipeline.js --verbose",
    "test:pipeline:continuous": "node test-pipeline.js --continuous",
    "test:benchmark": "node test-pipeline.js --benchmark-only",
    "test:sqlite": "node tests/sqlite_state_validator.js",
    "test:neo4j": "node tests/neo4j_state_validator.js",
    "validate:state": "npm run test:sqlite && npm run test:neo4j",
    "pipeline:dry-run": "node test-pipeline.js --dry-run",
    "pipeline:reliability": "node test-pipeline.js --repeat 5",
    "test:benchmark:master": "jest tests/benchmark/masterBenchmark.test.js --timeout=2100000",
    "test:benchmark:analysis": "node tests/benchmark/runBenchmarkAnalysis.js",
    "benchmark": "npm run test:benchmark:master",
    "benchmark:analysis": "npm run test:benchmark:analysis"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.6.2",
    "better-sqlite3": "^9.6.0",
    "bullmq": "^5.56.0",
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.2",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "ignore": "^7.0.5",
    "ioredis": "^5.6.1",
    "neo4j-driver": "^5.28.1",
    "prebuild-install": "^7.1.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "commander": "^12.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.0",
    "jest": "^29.7.0",
    "mock-fs": "^5.5.0",
    "zod": "^3.25.67"
  }
}
