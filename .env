STALE_CLEANUP_INTERVAL=600000
FAILED_CLEANUP_INTERVAL=1800000
COMPLETED_CLEANUP_INTERVAL=3600000
MAX_JOB_AGE=86400000
MAX_STALE_AGE=1800000
MAX_FAILED_RETENTION=100
MAX_COMPLETED_RETENTION=50
